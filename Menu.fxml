<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox alignment="CENTER" maxHeight="425.0" maxWidth="800.0" minHeight="425.0" minWidth="800.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MenuSceneController">
   <children>
      <VBox HBox.hgrow="ALWAYS">
         <children>
            <HBox alignment="CENTER" prefWidth="200.0">
               <children>
                  <Button fx:id="addButton" minWidth="100.0" mnemonicParsing="false" onMouseClicked="#addClicked" text="Add">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="editButton" minWidth="100.0" mnemonicParsing="false" onMouseClicked="#editClicked" text="Edit">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="deleteButton" minWidth="100.0" mnemonicParsing="false" onMouseClicked="#deleteClicked" text="Delete">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="refreshButton" minWidth="100.0" mnemonicParsing="false" onMouseClicked="#refreshClicked" text="Refresh Table">
                     <HBox.margin>
                        <Insets left="140.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="deliveriesButton" minWidth="100.0" mnemonicParsing="false" onMouseClicked="#deliveriesClicked" text="Pending Deliveries" />
               </children>
            </HBox>
            <TableView fx:id="inventoryTable" minWidth="300.0" onKeyReleased="#tableViewClicked" onMouseClicked="#tableViewClicked" prefWidth="556.0" />
         </children>
      </VBox>
   </children>
</HBox>
